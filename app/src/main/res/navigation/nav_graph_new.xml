<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph_new"
    app:startDestination="@id/loginFragment"
    app:defaultNavHost="true">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.dognutrionapp.ui.home.HomeFragment"
        android:label="Home" />

    <fragment
        android:id="@+id/cartFragment"
        android:name="com.example.dognutrionapp.ui.cart.CartFragment"
        android:label="Cart" />

    <fragment
        android:id="@+id/catalogFragment"
        android:name="com.example.dognutrionapp.ui.catalog.CatalogFragment"
        android:label="Catalog" />

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.dognutrionapp.ui.profile.ProfileFragment"
        android:label="Profile" />

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.dognutrionapp.ui.auth.LoginFragment"
        android:label="Login" />

    <fragment
        android:id="@+id/signupFragment"
        android:name="com.example.dognutrionapp.ui.auth.SignupFragment"
        android:label="Sign Up" />

    <fragment
        android:id="@+id/productDetailFragment"
        android:name="com.example.dognutrionapp.ui.catalog.ProductDetailFragment"
        android:label="Product Detail" />

    <argument
        android:name="product"
        app:argType="com.example.dognutrionapp.ui.models.Product" />

    <fragment
        android:id="@+id/adminDashboardFragment"
        android:name="com.example.dognutrionapp.ui.admin.AdminDashboardFragment"
        android:label="Admin Dashboard" />

    <fragment
        android:id="@+id/addProductFragment"
        android:name="com.example.dognutrionapp.ui.admin.AddProductFragment"
        android:label="Add New Product" />

    <fragment
        android:id="@+id/editProductFragment"
        android:name="com.example.dognutrionapp.ui.admin.EditProductFragment"
        android:label="Edit Product" />

    <fragment
        android:id="@+id/educationalContentFragment"
        android:name="com.example.dognutrionapp.ui.educational.EducationalContentFragment"
        android:label="Edit Product" />

    <fragment
        android:id="@+id/manageUsersFragment"
        android:name="com.example.dognutrionapp.ui.admin.ManageUsersFragment"
        android:label="Manage Users" />

    <fragment
        android:id="@+id/addUserFragment"
        android:name="com.example.dognutrionapp.ui.admin.AddUserFragment"
        android:label="Add New User" />

    <fragment
        android:id="@+id/updateProfileFragment"
        android:name="com.example.dognutrionapp.ui.profile.UpdateProfileFragment"
        android:label="Update Information" />

    <fragment
        android:id="@+id/orderManagementFragment"
        android:name="com.example.dognutrionapp.ui.profile.OrderManagementFragment"
        android:label="Order Information" />

    <fragment
        android:id="@+id/addPersonalInfoFragment"
        android:name="com.example.dognutrionapp.ui.profile.AddPersonalInfoFragment"
        android:label="Add Information" />

    <action
        android:id="@+id/action_homeFragment_to_cartFragment"
        app:destination="@id/cartFragment" />
    <action
        android:id="@+id/action_homeFragment_to_catalogFragment"
        app:destination="@id/catalogFragment" />
    <action
        android:id="@+id/action_homeFragment_to_educationalContentFragment"
        app:destination="@id/educationalContentFragment" />
    <action
        android:id="@+id/action_homeFragment_to_profileFragment"
        app:destination="@id/profileFragment" />
    <action
        android:id="@+id/action_loginFragment_to_signupFragment"
        app:destination="@id/signupFragment" />
    <action
        android:id="@+id/action_loginFragment_to_homeFragment"
        app:destination="@id/homeFragment" />
    <action
        android:id="@+id/action_signupFragment_to_loginFragment"
        app:destination="@id/loginFragment" />
    <action
        android:id="@+id/action_catalogFragment_to_productDetailFragment"
        app:destination="@id/productDetailFragment" />
    <action
        android:id="@+id/action_addProductFragment_to_adminDashboardFragment"
        app:destination="@id/adminDashboardFragment" />
    <action
        android:id="@+id/action_adminDashboardFragment_to_addProductFragment"
        app:destination="@id/addProductFragment" />
    <action
        android:id="@+id/action_adminDashboardFragment_to_editProductFragment"
        app:destination="@id/editProductFragment" />
    <action
        android:id="@+id/action_editProductFragment_to_adminDashboardFragment"
        app:destination="@id/adminDashboardFragment" />
    <action
        android:id="@+id/action_loginFragment_to_adminDashboardFragment"
        app:destination="@id/adminDashboardFragment" />
    <action
        android:id="@+id/action_adminDashboardFragment_to_manageUsersFragment"
        app:destination="@id/manageUsersFragment" />
    <action
        android:id="@+id/action_manageUsersFragment_to_addUserFragment"
        app:destination="@id/addUserFragment" />
    <action
        android:id="@+id/action_profileFragment_to_addPersonalInfoFragment"
        app:destination="@id/addPersonalInfoFragment" />
    <action
        android:id="@+id/action_profileFragment_to_updateProfileFragment"
        app:destination="@id/updateProfileFragment" />
    <action
        android:id="@+id/action_profileFragment_to_orderManagementFragment"
        app:destination="@id/orderManagementFragment" />
</navigation>